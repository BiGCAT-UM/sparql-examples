@prefix cito: <http://purl.org/spar/cito/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix ex: <https://bigcat-um.github.io/sparql-examples/WikidataRetractions/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix spex: <https://purl.expasy.org/sparql-examples/ontology#> .

ex:011 a sh:SPARQLExecutable,
       sh:SPARQLSelectExecutable ;
    rdfs:comment "Journals that cite retracted articles, sorted by citation count."@en ;
    sh:prefixes _:sparql_examples_prefixes ;
    sh:select """PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT ?journal ?journalLabel (COUNT(DISTINCT ?citesRetracted) AS ?count) WHERE {
  ?citesRetracted wdt:P2860 / wdt:P31 wd:Q45182324 ;
                  wdt:P1433 ?journal .
  ?journal rdfs:label ?journalLabel . FILTER (LANG(?journalLabel) = "en") .
} GROUP BY ?journal ?journalLabel 
  ORDER BY DESC(?count)
  LIMIT 100""";
    schema:target <https://qlever.dev/api/wikidata> ;
    dc:contributor "Egon Willighagen" ;
    dcterms:license <http://creativecommons.org/publicdomain/zero/1.0/> ;
    cito:usesMethodIn <https://www.wikidata.org/wiki/Wikidata:WikiProject_Retractions/SPARQL> ;
    schema:keywords "retraction" .
